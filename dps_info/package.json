    "jupyterlab-extension"
  ],
  "homepage": "https://github.com/MAAP-Project/maap-jupyter-ide/tree/master/dps_info",
  "bugs": {
    "url": "https://github.com/MAAP-Project/maap-jupyter-ide/tree/master/dps_info/issues"
  },
  "license": "BSD-3-Clause",
