    "@phosphor/widgets": "^1.9.3",
    "@types/jquery": "^3.3.31",
    "@types/node": "^11.15.2",
    "jquery": "^3.4.1",
    "jupyterlab_toastify": "^2.3.2",
    "require": "^2.4.20",
    "tsc": "^1.20150623.0",
    "xml-formatter": "^1.1.0"
