{
  "private": true,
  "name": "submit_jobs",
  "version": "0.0.1",
  "description": "UI form for users to submit jobs to HySDS/DPS",
  "author": "Elizabeth Yam",
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
